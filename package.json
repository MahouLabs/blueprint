{
  "name": "blueprint",
  "version": "1.3.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "bun clean && node ./server.mjs",
    "build": "bun clean && remix vite:build",
    "start": "cross-env NODE_ENV=production node ./server.mjs",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true node server.mjs",
    "test": "vitest",
    "typecheck": "tsc",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --apply",
    "format": "biome format --write .",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "coverage": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:install": "npx playwright install --with-deps chromium",
    "clean": "rimraf build",
    "nuke": "bun clean && rimraf node_modules package-lock.json && bun i",
    "act": "doppler run -- act -s FLY_API_TOKEN --container-architecture linux/amd64",
    "ui:add": "bunx shadcn-ui@latest add",
    "ui:diff": "bunx shadcn-ui@latest diff"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@remix-run/css-bundle": "^2.4.1",
    "@remix-run/express": "^2.4.1",
    "@remix-run/node": "^2.4.1",
    "@remix-run/react": "^2.4.1",
    "@supabase/ssr": "^0.0.10",
    "@supabase/supabase-js": "^2.39.1",
    "chalk": "^5.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "isbot": "^4.1.0",
    "lucide-react": "^0.309.0",
    "morgan": "^1.10.0",
    "next-themes": "^0.2.1",
    "react": "18.3.0-canary-c5b937576-20231219",
    "react-dom": "18.3.0-canary-c5b937576-20231219",
    "remix-hook-form": "^3.2.3",
    "sonner": "^1.3.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@flydotio/dockerfile": "^0.5.0",
    "@playwright/test": "^1.40.1",
    "@remix-run/dev": "^2.4.1",
    "@remix-run/testing": "^2.4.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "all-contributors-cli": "^6.26.1",
    "autoprefixer": "^10.4.16",
    "jsdom": "^23.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "remix-development-tools": "^3.6.2",
    "remix-flat-routes": "^0.6.4",
    "rimraf": "^5.0.5",
    "supabase": "^1.127.3",
    "tailwindcss": "^3.4.0",
    "typescript": "5.1.6",
    "vite": "^5.0.10",
    "vite-tsconfig-paths": "^4.2.2",
    "vitest": "^1.1.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "eslintIgnore": [
    "node_modules",
    "build",
    "public/build"
  ]
}
